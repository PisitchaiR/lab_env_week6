pipeline {
    agent any
    stages{
        stage('Welcome to master'){
            steps{
                echo 'Welcome to master'
            }
        }

        stage('tigger to slave job')
        {
            environment{
                name = "thun"
                age = "20"
            }
            steps{
                build job: 'slave', parameters: [string(name: 'name', value: env.name), string(name: 'age', value: env.age)]
            }
        }
    }
}